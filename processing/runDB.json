        "daq":"ORCA",
        "digitizer":"ORSIS3302DecoderForEnergy"
      },
      "raw_to_dsp_options":{
        "clk" : 100e6,
        "fit_bl" : {"ihi":500, "order":1},
        "blsub" : {},
        "dADC": {},
        "pz" : {"decay":82},
